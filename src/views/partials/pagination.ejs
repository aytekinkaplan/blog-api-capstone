<div class="flex justify-between items-center">
  <% if (page > 1) { %>
  <a
    href="?page=<%= page - 1 %>"
    class="bg-gray-200 text-gray-700 px-4 py-2 rounded-lg hover:bg-gray-300"
    >&laquo; Previous</a
  >
  <% } %> <% if (page < totalPages) { %>
  <a
    href="?page=<%= page + 1 %>"
    class="bg-gray-200 text-gray-700 px-4 py-2 rounded-lg hover:bg-gray-300"
    >Next &raquo;</a
  >
  <% } %>
</div>
